{
  "schema_version": 1,

  "quilt_loader": {

    "group": "net.sydokiddo",
    "id": "chrysalis",
    "version": "${version}",

    "metadata": {
      "name": "Chrysalis",
      "description": "A library mod and shared API for all of Sydokiddo's Minecraft Quilt mods!",
      "contributors": {
        "Sydokiddo": "Owner",
        "Shock Micro": "Emissive Core Shaders",
        "Discohund": "Emissive Core Shaders"
      },
      "contact": {
        "homepage": "https://modrinth.com/mod/chrysalis",
        "issues": "https://github.com/Sydokiddo/chrysalis/issues",
        "sources": "https://github.com/Sydokiddo/chrysalis"
      },
      "license": "MIT",
      "icon": "assets/chrysalis/icon.png"
    },

    "intermediate_mappings": "net.fabricmc:intermediary",

    "entrypoints": {
      "init": "net.sydokiddo.chrysalis.Chrysalis",
      "client": "net.sydokiddo.chrysalis.client.ChrysalisClient"
    },

    "depends": [
      {
        "id": "quilt_loader",
        "versions": ">=0.19.1"
      },
      {
        "id": "quilted_fabric_api",
        "versions": ">=7.0.2"
      },
      {
        "id": "minecraft",
        "versions": ">=1.20"
      }
    ]
  },

  "mixin": "chrysalis.mixins.json",
  "access_widener": "chrysalis.accesswidener"
}
