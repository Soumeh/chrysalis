{
  "schemaVersion": 1,
  "id": "chrysalis",
  "version": "${version}",

  "name": "Chrysalis",
  "description": "A library mod and shared API for all of Sydokiddo's Minecraft Fabric mods!",
  "authors": [
    "Sydokiddo"
  ],
  "contact": {},
  "license": "MIT",
  "icon": "assets/chrysalis/icon.png",
  "accessWidener": "chrysalis.accesswidener",
  "environment": "*",

  "entrypoints": {
    "main": [
      "net.sydokiddo.chrysalis.Chrysalis"
    ],
    "client": [
      "net.sydokiddo.chrysalis.client.ChrysalisClient"
    ]
  },
  "mixins": [
    "chrysalis.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.19",
    "fabric-api": "*",
    "minecraft": "~1.19.4",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  },
  "breaks": {},
  "custom": {}
}
